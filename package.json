{
  "name": "nuxt3-template",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.13.1",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "build": "nuxt build",
    "build:gh-pages": "nuxt build --dotenv .env.gh-pages",
    "dev": "nuxt dev",
    "dev:gh-pages": "nuxt dev --dotenv .env.gh-pages",
    "generate": "nuxt generate",
    "generate:gh-pages": "nuxt generate --dotenv .env.gh-pages",
    "preview": "nuxt preview",
    "preview:gh-pages": "nuxt preview --dotenv .env.gh-pages",
    "postinstall": "nuxt prepare",
    "prepare": "husky install && husky set .husky/commit-msg \"npm exec --no -- commitlint --edit $1\" && husky set .husky/pre-merge-commit \"npm run eslint && npm run typelint\" && husky set .husky/pre-push \"npm run eslint && npm run typelint\"",
    "eslint": "eslint --ext .ts,.js,.vue .",
    "eslint:fix": "eslint --ext .ts,.js,.vue . --fix",
    "typelint": "nuxi typecheck",
    "customize": "node customize"
  },
  "devDependencies": {
    "@commitlint/cli": "18.2.0",
    "@commitlint/config-conventional": "18.1.0",
    "@inquirer/prompts": "7.3.1",
    "@nuxt/icon": "1.10.3",
    "@nuxtjs/eslint-config-typescript": "12.1.0",
    "@nuxtjs/i18n": "8.0.0",
    "@nuxtjs/tailwindcss": "6.13.1",
    "@pinia/nuxt": "0.10.1",
    "@typescript-eslint/eslint-plugin": "8.46.0",
    "@typescript-eslint/parser": "8.46.0",
    "@vee-validate/i18n": "4.15.0",
    "@vee-validate/nuxt": "4.15.0",
    "@vee-validate/rules": "4.15.0",
    "@vee-validate/zod": "4.15.0",
    "@vueuse/core": "12.5.0",
    "@vueuse/nuxt": "12.5.0",
    "eslint": "8.57.1",
    "eslint-import-resolver-typescript": "^3.10.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-nuxt": "4.0.0",
    "eslint-plugin-tailwindcss": "3.18.0",
    "eslint-plugin-vue": "9.32.0",
    "flat": "6.0.1",
    "husky": "8.0.3",
    "nuxt": "^3.17.7",
    "pinia": "3.0.1",
    "spreadsheet-to-json": "^2.0.0",
    "typescript": "5.7.3",
    "unhead": "2.0.19",
    "vee-validate": "4.15.0",
    "vue-tsc": "2.2.0",
    "zod": "3.24.2"
  },
  "overrides": {
    "vue": "latest",
    "axios": "^1.7.7"
  }
}
